{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FileSettings": {
    "MaxFileSizeInMB": 1024,
    "UploadPath": "uploads",
    "AllowedExtensions": [
      ".pdf", ".doc", ".docx", ".xls", ".xlsx", 
      ".png", ".jpg", ".jpeg", ".gif", ".txt", 
      ".zip", ".rar"
    ]
  }
}
